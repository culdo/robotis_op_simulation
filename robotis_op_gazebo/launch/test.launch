<launch>

   <param name="use_sim_time" value="true" />

   <param name="robot_description" textfile="$(find robotis_op_description)/urdf/robotis_op.urdf" />  

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find robotis_op_gazebo)/worlds/test.world"/>
    <arg name="paused" value="true"/>
  </include>

  
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find robotis_op_description)/urdf/robotis_op.urdf -urdf -model robot_description -x 2" />


  <include file="$(find robotis_op_simulation_control)/launch/robotis_op_position_control.launch" />






</launch>
